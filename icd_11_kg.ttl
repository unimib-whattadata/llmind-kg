icd-kg-entity:Synthetic_cannabinoid_dependence_early_full_remission a icd-kg:Synthetic_cannabinoid_dependence ;
    icd-schema:exclusion icd-kg-entity:Episode_of_harmful_synthetic_cannabinoids,
