        icd-kg:Volatile_inhalants ;
    icd-kg:hasDiagnosticRequirements <http://icd_kg/6/entity/-_all_diagnostic_requirements_for_6d70_delirium_are_met.>,
        <http://icd_kg/6/entity/The_diagnostic_requirements_for_6d70.2_delirium_multiple_etiological_factors_are_not_met._%2A%2A%2Anote%3A%2A%2A%2A_see_6d70_delirium_for_other_cddr_sections_including_additional_clinical_features_boundary_with_normality_%28threshold%29_course_features_presentations_culturerelated_features_and_boundaries_with_other_and_conditions_%28differential_diagnosis%29.>,
